package com.freshitaly.rest.controller;

public class RestTest {

	private String id;
	private String text;
	
	public RestTest(){}

	public RestTest(String id, String value){
		this.id = id;
		this.text = value;
	}

	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getValue() {
		return text;
	}
	public void setValue(String value) {
		this.text = value;
	}
	

}
